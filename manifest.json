{
  "manifest_version": 2,

  "name": "SuikaBuy",
  "description": "just for 11.11",
  "version": "1.0",

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "background",
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
